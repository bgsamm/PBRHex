  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Command Parser Generator",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}\\commands.py",
      "args": [
        "${workspaceFolder}\\commands\\commands.json",
        "${workspaceFolder}\\CommandParserAuto.cs",
        "PBRHex.CLI",
        "CommandParser"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${fileDirname}"
    }
  ]
}